<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style x:Key="StackPanelHeaderLabel">
        <Setter Property="Control.HorizontalAlignment" Value="Center"/>
        <Setter Property="Control.VerticalAlignment" Value="Center"/>
        <Setter Property="Control.FontSize" Value="20"/>
    </Style>
    <Style x:Key="StackPanelProperty">
        <Setter Property="Control.FontSize" Value="15"/>
        <Setter Property="Control.Margin" Value="20, 5"/>
    </Style>
    <Style x:Key="StackPanelSaveButton">
        <Setter Property="Control.HorizontalAlignment" Value="Right"/>
        <Setter Property="Control.Height" Value="40"/>
        <Setter Property="Control.Width" Value="150"/>
        <Setter Property="Control.Margin" Value="20, 5"/>
    </Style>
    <Style x:Key="StackPanelErrorLabel">
        <Setter Property="Control.FontSize" Value="15"/>
        <Setter Property="Control.Margin" Value="20, 5"/>
        <Setter Property="TextElement.Foreground" Value="#D33031"/>
    </Style>
    <Style x:Key="StackPanelPassLabel">
        <Setter Property="Control.FontSize" Value="15"/>
        <Setter Property="Control.Margin" Value="20, 5"/>
        <Setter Property="TextElement.Foreground" Value="Green"/>
    </Style>
</ResourceDictionary>