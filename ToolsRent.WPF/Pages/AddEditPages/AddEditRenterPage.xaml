<Page x:Class="ToolsRent.WPF.Pages.AddEditPages.AddEditRenterPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      mc:Ignorable="d"
      Title="AddEditRenterPage" Height="450" Width="800">
    <Grid>
        <StackPanel Name="RenterData">
            <Label Content="Данные клиента:" Style="{StaticResource StackPanelHeaderLabel}"/>
            
            <Label Content="Фамилия клиента:" Style="{StaticResource StackPanelProperty}"/>
            <TextBox Name="TbxSurname" Style="{StaticResource StackPanelProperty}"/>
        </StackPanel>
    </Grid>
</Page>
